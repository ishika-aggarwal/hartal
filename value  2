#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
typedef long long ll;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        ll r;
        cin>>r;
        vector<ll> a(r);
        for(ll i=0; i<r; i++)
        {
            cin>>a[i];
        }
          sort(a.begin(),a.end());
          
          cout<<a[(r-1)/2]<<endl;
        
    }
      
    return 0;
}
